[sub_resource type="GDScript" id="GDScript_lbogj"]
resource_name = "player_movement"
script/source = "extends CharacterBody2D

const SPEED = 200

#Please remember to add Area2D, Direction etc to this so it can interact with coral...follow Nathan's video
@onready var actionable_finder: Area2D = $characterdirection/ActionableFinder
@export var move_speed : float = 100
@onready var topdownfish_animation = get_node(\"AnimationPlayer\")
var last_direction = Vector2.ZERO
var input_vector: Vector2 = Vector2.ZERO

func _physics_process(delta):
	var input_direction = Vector2(
		Input.get_action_strength(\"ui_right\") - Input.get_action_strength(\"ui_left\"),
		Input.get_action_strength(\"ui_down\") - Input.get_action_strength(\"ui_up\")
	)
	velocity = input_direction * delta
	
	if input_direction != Vector2.ZERO:
		if abs(input_direction.x) > abs(input_direction.y): # Horizontal movement is stronger
			if input_direction.x < 0:
				get_node(\"AnimatedSprite2D\").flip_h = true
				velocity.x = input_direction.y * SPEED
			else:
				get_node(\"AnimatedSprite2D\").flip_h = false
				
			velocity.x = input_direction.x * SPEED
			velocity.y = 0
			topdownfish_animation.play(\"walk_right\")
			last_direction = Vector2(input_direction.x, 0)
			
		else: # Vertical movement is stronger
			if input_direction.y < 0:
				velocity.y = input_direction.y * SPEED
				topdownfish_animation.play(\"walk_up\")
				last_direction = Vector2(0, 1)
			else:
				topdownfish_animation.play(\"walk_down\")
				velocity.y = input_direction.y * SPEED
				last_direction = Vector2(0, -1)

	else: # No movement
		if last_direction.x < 0:
			topdownfish_animation.play(\"Idle_Left\")
		elif last_direction.x > 0:
			topdownfish_animation.play(\"Idle_Right\")
		elif last_direction.y < 0:
			topdownfish_animation.play(\"Idle_Down\")
		elif last_direction.y > 0:
			topdownfish_animation.play(\"Idle_Up\")

	move_and_slide()

func _unhandled_input(_event: InputEvent) -> void:
	if Input.is_action_just_pressed(\"ui_accept\"):
		var actionables = actionable_finder.get_overlapping_areas()
		if actionables.size() > 0:
			actionables[0].action()
			return
	
	var x_axis: float = Input.get_axis(\"ui_left\", \"ui_right\")
	var y_axis: float = Input.get_axis(\"ui_up\", \"ui_down\")
	if x_axis:
		input_vector = x_axis * Vector2.RIGHT
	
	elif y_axis:
		input_vector = y_axis * Vector2.DOWN
	else:
		input_vector = Vector2.ZERO

"

